.head
  width 100%
  background-color: #fff;
  border-bottom 1px solid #e5e5e5
  padding 2.5px
  .head_container
    box-sizing border-box
    width 1200px
    margin 0 auto
    display flex
    justify-content space-between
    font: 16px/1.5 latoregular, Verdana, sans-serif;
    padding 0 15px
    .logo
      height  65px
      img
        display block
        width 240px
        height 40px
        margin 8px 0
    .head_nav
      display flex
      align-items center
      .position
        margin-right 50px
      li
        padding 10px 15px
        text-align center
        font-weight 700
        a:hover
          color #e94840;
        img
          width 20px
          height 20px
          vertical-align middle
      .shanghu
        padding 4px 6px
        margin-top 5px
        margin-left 15px
        background: #e94840;
        border-radius 5px
        color #fff

.carousel-example-generic
  .flicker-example
    position relative
    .swiper-container
      .swiper-wrapper
        margin 0 auto
        height 420px
        position relative
       li
         float left
         width 100%
         height 420px
         overflow hidden
      .swiper-pagination
        .swiper-pagination-bullet
          width 10px
          height 10px
        .swiper-pagination-bullet-active
          background-color: #fff;
    .banner-nav_container
      width 1200px
      height 420px
      margin-top -420px
      margin-right auto
      margin-left auto
      .banner-nav
        z-index 10
        position absolute
        top 0
        width 180px
        height 420px
        background-color: rgba(0,0,0,0.6)
        ul
          .navname
            height 42px
            line-height 42px
            font-size 16px
            font-weight bold
            cursor: pointer;
            padding-left: 45px;
            a
              display: block;
              width: 100%;
              height: 100%;
              color #fff
              i
                display: inline-block;
                width: 70px;
                text-align: justify;
                text-align-last: justify;
              img
                vertical-align middle
                margin-left 25px
            .none_list
              position absolute
              top 0
              left 180px
              width 180px
              height 420px
              background-color: rgba(255,255,255,0.3)
              li
                height: 42px;
                line-height: 42px;
                color: #fff;
                font-size: 16px;
                position: relative;
                font-weight: bold;
                cursor: pointer;
                padding-left: 45px;
                a
                  color #121212
