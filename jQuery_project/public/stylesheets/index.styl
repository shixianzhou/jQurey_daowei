.active
  a
    color #e94840
    background-color: #fff;
.head
  transition all 0.5s
  width 100%
  background-color: #fff;
  border-bottom 1px solid #e5e5e5
  padding 2.5px
  box-sizing border-box
  .head_container
    box-sizing border-box
    width 1200px
    margin 0 auto
    display flex
    justify-content space-between
    font: 16px/1.5 latoregular, Verdana, sans-serif;
    padding 0 15px
    .logo
      height  65px
      img
        display block
        width 240px
        height 40px
        margin 8px 0
    .head_nav
      display flex
      align-items center
      .position
        margin-right 50px
      li
        padding 10px 15px
        text-align center
        font-weight 700
        a:hover
          color #e94840;
          transition all 2s
        img
          width 20px
          height 20px
          vertical-align middle
      .shanghu
        padding 4px 6px
        margin-top 5px
        margin-left 15px
        background: #e94840;
        border-radius 5px
        color #fff

.carousel-example-generic
  .flicker-example
    position relative
    .swiper-container
      .swiper-wrapper
        margin 0 auto
        height 420px
        position relative
       li
         float left
         width 100%
         height 420px
         overflow hidden
      .swiper-pagination
        .swiper-pagination-bullet
          width 10px
          height 10px
        .swiper-pagination-bullet-active
          background-color: #fff;
    .banner-nav_container
      width 1200px
      height 420px
      margin-top -420px
      margin-right auto
      margin-left auto
      .banner-nav
        z-index 10
        position absolute
        top 0
        width 180px
        height 420px
        background-color: rgba(0,0,0,0.5)
        ul
          .navname
            height 42px
            line-height 42px
            font-size 16px
            font-weight bold
            cursor: pointer;
            padding-left: 45px;
            a
              display: block;
              width: 100%;
              height: 100%;
              color #fff
              i
                display: inline-block;
                width: 70px;
                text-align: justify;
                text-align-last: justify;
              img
                vertical-align middle
                margin-left 25px
            &:hover ul
              display block
            .none_list
              display none
              position absolute
              top 0
              left 180px
              width 180px
              height 420px
              background-color: rgba(255,255,255,0.9)
              li
                height: 42px;
                line-height: 42px;
                color: #fff;
                font-size: 16px;
                position: relative;
                font-weight: bold;
                cursor: pointer;
                padding-left: 45px;
                a
                  color #121212
.box1
  width 100%
  padding-bottom 20px
  &:nth-child(even){background: #fbf8f8;}
  overflow hidden
  .fff
    width 1200px
    margin 0 auto
    padding 50px 0 0
    clear both
    .servicebody_top
      text-align center
      .serviceIndex
        color #3d3838
        font-weight 800
        font-size 28px
        font-family "Microsoft YaHei UI"
      .serviceType
        display block
        font-family "幼圆"
        font-size 22px
        margin-top 15px
        color #3d3838
    .servicebody_bottom
      box-sizing border-box
      .tagsListHeader
        text-align right
    .servicebody_bottom
      .tagsListHeader
        margin-bottom 20px
        span
          color: #898989;
          font-size 18px
          font-family "幼圆"
      .row
        width 1200px
        .member-container
          box-sizing border-box
          float left
          padding 10px
          width 230px
          border 1px solid #eaeaea
          background #fff
          cursor pointer
          height 350px
          margin 0 5px
          &:hover
            box-shadow: 5px 5px 15px #ccc;
          img
            width 100%
            vertical-align middle
          .serviceName
            font-size 16px
            padding 5px 0
            font-weight 800
            color #000000
            height 22px
            overflow hidden
            width 101%
            white-space: nowrap;
            text-overflow: ellipsis;
          .description
            color: #929292;
            overflow: hidden;
            text-overflow: ellipsis;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            word-break: break-all;
            font-size: 13px;
            line-height: 18px;
            height: 58px;
          .price
            font-size 14px
            padding 5px 5px
            font-weight 600
            height 20px
            line-height 20px
            color: #fb474f;
            span
              font-size 25px
            .btn
              float right
              padding 0px
              width 76px
              height 24px
              line-height 22px
              text-align center
              background: #fb474f;
              border-radius 5px
              color #fff
              font-size 14px
