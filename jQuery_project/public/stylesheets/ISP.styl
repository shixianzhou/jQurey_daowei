.servi
  width: 100%;
  padding-top: 20px;
  background: #fff;
  height: 66px;
  font-family: "微软雅黑";
  ul
    width: 420px;
    margin: 0 auto;
    overflow hidden
    li
      width: 33%;
      float: left;
      text-align: center;
      color: #1d1515;
      font-size: 18px;
      img
        margin-right 10px
        vertical-align middle
.footer
  width 100%
  height 170px
  clear both
  box-sizing border-box
  padding-top 50px
  background #fff
  dl
    width: 1200px;
    margin: 0 auto;
    overflow hidden
    dt
      float left
      font-family "微软雅黑"
      font-size 18px
      font-weight 700
      display block
      line-height 1.42
    dd
      margin-left 30px
      width 1090px
      line-height 30px
      float left
      display block
      a
        font-size 16px
        color: #898989;
        display inline-block
        width 8.6%
        text-align center
        font-weight 200
        cursor: pointer;
        font-family: "微软雅黑";
.beian
  width 100%
  text-align center
  padding-top 50px
  clear both
  box-sizing border-box
  color #333333
  background-color: #fbf8f8;
  .htit
    position: relative;
    bottom: 20px;
    text-align: left;
    font-weight: bold;
    width: 1200px;
    margin: 0 auto;
    background: none;
    color: #1d1515;
    font-size: 18px;
  .hezuo
    width 1200px
    margin 0 auto
    overflow hidden
    li
      float left
      width 10%
      height 60px
      text-align center
      background-color: #fff;
      margin-left 1%
      line-height 55px
      cursor pointer
      img
        vertical-align middle
        border 0
        width 100px
.banquan
  width 100%
  height 40px
  padding-top 0px
  background-color: #fbf8f8;
  p
    width 1200px
    margin 0 auto
    text-align left
    color #898989
    line-height 40px