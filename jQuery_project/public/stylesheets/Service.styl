body
  background-color: #fbf8f8;
.active
  a
    color #e94840
    background-color: #fff;
.head
  transition all 0.5s
  width 100%
  background-color: #fff;
  border-bottom 1px solid #e5e5e5
  padding 2.5px
  box-sizing border-box
  .head_container
    box-sizing border-box
    width 1200px
    margin 0 auto
    display flex
    justify-content space-between
    font: 16px/1.5 latoregular, Verdana, sans-serif;
    padding 0 15px
    .logo
      height  65px
      img
        display block
        width 240px
        height 40px
        margin 8px 0
    .head_nav
      display flex
      align-items center
      .position
        margin-right 50px
      li
        padding 10px 15px
        text-align center
        font-weight 700
        a:hover
          color #e94840;
          transition all 2s
        img
          width 20px
          height 20px
          vertical-align middle
      .shanghu
        padding 4px 6px
        margin-top 5px
        margin-left 15px
        background: #e94840;
        border-radius 5px
        color #fff

.h2
  line-height 85px
  width 1200px
  margin 0 auto
  font-size 18px
  font-weight:600;
  color #333
.service-center
  width 1200px
  margin 0 auto
  clear both
  .centerleft
    width 970px
    float left
    margin-bottom 50px
    box-sizing border-box
    .tabdiv
      overflow hidden
    .ulactivety
      display block
      overflow hidden
      li:hover
        box-shadow: 5px 5px 10px #ccc;
      li
        width 232px
        height 300px
        border 1px solid #eaeaea
        padding 10px
        float left
        margin-right 10px
        margin-bottom 10px
        background-color: #fff;
        cursor pointer
        box-sizing border-box
        img
          vertical-align middle
          width 210px
          height 210px
        p
          height 26px
          line-height 26px
          margin 0
          white-space nowrap
          overflow hidden
          text-overflow ellipsis
          img
            width 18px
            height 17px
            border 0
            vertical-align middle
            margin-right 6px
            margin-bottom 5px
          b
            font-size 16px
            font-weight:700;
        .pp
          height 26px
          line-height 26px
          margin 0
          white-space nowrap
          overflow hidden
          text-overflow ellipsis
          font-size 12px
          color #929292
          i
            float right
  .getServiceList
    width 100%
    height 152px
    margin 0 auto
    text-align center
    display block
    box-sizing border-box
    span
      width 200px
      height 50px
      line-height 50px
      border 1px solid #e5e5e5
      border-radius 5px
      margin 50px 5px
      background-color: #fff;
      display inline-block
      font-size 16px
      color #b1b1b1
      cursor pointer
  .centerright
    float right
    width 230px
    box-sizing border-box
    img
      vertical-align middle
      cursor pointer
      border 0
      width 230px
    .erweima
      width 230px
      height 290px
      background: url(../pages/Servive/images/tuijianfuwushang_06.jpg) no-repeat center;
      margin-top 10px
      img
        width 130px
        height auto
        margin 50px 0 0 50px
        vertical-align middle
        border 0
        box-sizing border-box

.baozhang
  text-align: center;
  width: 100%;
  padding-top: 60px;
  background: #fff;
  clear: both;
  h2
    font-size 30px
    font-family "微软雅黑"
    font-weight 800
    margin-top: 20px;
    margin-bottom: 10px;
    i
      color: #fb474f;
  p
    font-size 22px
    color: #3d3838;
    margin 0
.servi
  width: 100%;
  padding-top: 20px;
  background: #fff;
  height: 66px;
  font-family: "微软雅黑";
  ul
    width: 420px;
    margin: 0 auto;
    overflow hidden
    li
      width: 33%;
      float: left;
      text-align: center;
      color: #1d1515;
      font-size: 18px;
      img
        margin-right 10px
        vertical-align middle
.footer
  width 100%
  height 170px
  clear both
  box-sizing border-box
  padding-top 50px
  background #fff
  dl
    width: 1200px;
    margin: 0 auto;
    overflow hidden
    dt
      float left
      font-family "微软雅黑"
      font-size 18px
      font-weight 700
      display block
      line-height 1.42
    dd
      margin-left 30px
      width 1090px
      line-height 30px
      float left
      display block
      a
        font-size 16px
        color: #898989;
        display inline-block
        width 8.6%
        text-align center
        font-weight 200
        cursor: pointer;
        font-family: "微软雅黑";
.beian
  width 100%
  text-align center
  padding-top 50px
  clear both
  box-sizing border-box
  color #333333
  background-color: #fbf8f8;
  .htit
    position: relative;
    bottom: 20px;
    text-align: left;
    font-weight: bold;
    width: 1200px;
    margin: 0 auto;
    background: none;
    color: #1d1515;
    font-size: 18px;
  .hezuo
    width 1200px
    margin 0 auto
    overflow hidden
    li
      float left
      width 10%
      height 60px
      text-align center
      background-color: #fff;
      margin-left 1%
      line-height 55px
      cursor pointer
      img
        vertical-align middle
        border 0
        width 100px
.banquan
  width 100%
  height 40px
  padding-top 0px
  background-color: #fbf8f8;
  p
    width 1200px
    margin 0 auto
    text-align left
    color #898989
    line-height 40px